{
  "name": "bennys-theme",
  "displayName": "Benny's Theme",
  "description": "My personal theme with Color Theme and Product Icons (Intellisense)",
  "publisher": "bernardoamorim7",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/bernardoamorim7/bennys-theme"
  },
  "icon": "icon.ico",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Benny's Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/bennys-color-theme.json"
      },
      {
        "label": "Benny's Theme Adwaita Light",
        "uiTheme": "vs",
        "path": "./themes/bennys-color-theme-adwaita-light.json"
      },
      {
        "label": "Benny's Theme Adwaita Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/bennys-color-theme-adwaita-dark.json"
      }
    ],
    "productIconThemes": [
      {
        "id": "bennys-product-icons-theme",
        "label": "Benny's Product Icons Theme",
        "path": "./producticons/bennys-product-icons-theme.json"
      }
    ],
    "configurationDefaults": {
      "[go]": {
        "editor.semanticHighlighting.enabled": true
      },
      "[java]": {
        "editor.semanticHighlighting.enabled": true
      },
      "[python]": {
        "editor.semanticHighlighting.enabled": true
      },
      "[rust]": {
        "editor.semanticHighlighting.enabled": true
      }
    }
  }
}